{
	"compilerOptions": {
		"outDir": "./dist/",
		"baseUrl": ".",
		"paths": {
			"asmb/*": [
				"assembly-script/*"
			]
		},
		"sourceMap": true,
		"module": "commonjs",
		"target": "es6",
		"jsx": "react",
		"allowJs": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"tests/**/*",
		"assembly-script/**/*"
	]
}